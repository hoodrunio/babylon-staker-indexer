{
  "name": "babylon-indexer",
  "version": "1.0.0",
  "description": "Babylon Protocol Transaction Indexer",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "db:setup": "ts-node src/database/setup.ts",
    "db:migrate": "ts-node src/database/setup.ts",
    "db:reset": "ts-node src/database/setup.ts --reset",
    "migrate-public-keys": "ts-node src/scripts/migrate-staker-public-keys.ts",
    "test:decoder": "ts-node src/test-decoder.ts",
    "test:protobuf-decoder": "ts-node src/test-protobuf-decoder.ts",
    "get-tx": "ts-node src/scripts/get-transactions.ts"
  },
  "dependencies": {
    "@cosmjs/crypto": "^0.33.0",
    "@cosmjs/encoding": "^0.33.0",
    "@cosmjs/proto-signing": "^0.33.0",
    "@cosmjs/stargate": "^0.33.0",
    "@types/bitcoinjs-lib": "^4.0.1",
    "@types/cors": "^2.8.17",
    "@types/eventsource": "^1.1.15",
    "@types/ioredis": "^4.28.10",
    "@types/knex": "^0.15.2",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@types/ws": "^8.5.13",
    "async-mutex": "^0.5.0",
    "axios": "^1.7.9",
    "bech32": "^2.0.0",
    "bitcoinjs-lib": "^6.1.7",
    "chalk": "^4.1.2",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "cosmjs-types": "^0.9.0",
    "dotenv": "^16.4.7",
    "eventsource": "^3.0.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "ioredis": "^5.4.2",
    "knex": "^3.1.0",
    "long": "^5.3.1",
    "mongodb": "^6.3.0",
    "mongoose": "^8.1.1",
    "node-fetch": "^3.3.2",
    "openapi-types": "^12.1.3",
    "puppeteer": "^24.3.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "tiny-secp256k1": "^2.2.3",
    "uuid": "^11.0.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.28.1",
    "@bufbuild/protobuf": "^1.10.0",
    "@bufbuild/protoc-gen-es": "^1.10.0",
    "@types/chai": "^5.0.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/mocha": "^10.0.10",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.17.12",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
