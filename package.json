{
  "name": "babylon-indexer",
  "version": "1.0.0",
  "description": "Babylon Protocol Transaction Indexer",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
